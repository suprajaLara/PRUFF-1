<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.pruffap">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.google.pruffap.ForgotReset"></activity>
        <activity android:name="com.google.pruffap.WaitPage" />
        <activity android:name="com.google.pruffap.FinalRegister" />
        <activity android:name="com.google.pruffap.Philos" />
        <activity android:name="com.google.pruffap.Work" />
        <activity android:name="com.google.pruffap.About" />
        <activity android:name="com.google.pruffap.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.google.pruffap.OneTimePassword" />
        <activity
            android:name="com.google.pruffap.FirstOpenActivity"
            android:label="@string/title_activity_first_open"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.google.pruffap.Register" />
        <activity android:name="com.google.pruffap.MainActivity" />
    </application>

</manifest>